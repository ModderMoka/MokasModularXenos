<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!--======================================================================================-->

  <TraderKindDef>
    <defName>Caravan_Psy_ShamanMerchant</defName>
    <label>psychic shaman merchant</label>
    <stockGenerators>
      
      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>750~1200</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Pemmican</thingDef>
        <countRange>150~300</countRange>
      </li>
      
      <li Class="StockGenerator_Category">
        <categoryDef>Textiles</categoryDef>
        <thingDefCountRange>1~2</thingDefCountRange>
        <totalPriceRange>300~700</totalPriceRange>
      </li>

      <!-- Drugs -->

      <li Class="StockGenerator_Tag">
        <tradeTag>PrimMyst</tradeTag>
        <thingDefCountRange>2~4</thingDefCountRange>
        <countRange>30~40</countRange>
      </li>

      <li MayRequire="xrushha.EldersFaction" Class="StockGenerator_Tag">
        <tradeTag>AnimaDrug</tradeTag>
        <thingDefCountRange>2~4</thingDefCountRange>
        <countRange>30~40</countRange>
      </li>
    
      <li MayRequire="xrushha.EldersFaction" Class="StockGenerator_Tag">
        <tradeTag>Sac</tradeTag>
        <thingDefCountRange>1</thingDefCountRange>
        <countRange>1</countRange>
      </li>

      <!-- Weapons -->
      <li Class="StockGenerator_Tag">
        <tradeTag>AnimaWeapons</tradeTag>
        <thingDefCountRange>1~4</thingDefCountRange>
        <countRange>1~3</countRange>
      </li>
      
      <!-- Apparel -->
      <li Class="StockGenerator_Tag">
        <tradeTag>AnimaApparel</tradeTag>
        <thingDefCountRange>1~4</thingDefCountRange>
        <countRange>1~3</countRange>
      </li>
      <!-- none -->

      <!-- Implants -->
      <li Class="StockGenerator_Tag">
        <tradeTag>Astralinks</tradeTag>
        <thingDefCountRange>1~3</thingDefCountRange>
        <countRange>1~2</countRange>
      </li>
      <!-- none -->

      <!-- Buildings -->
      <!-- none -->

      <!-- Exotic -->
      <li Class="StockGenerator_Tag">
        <tradeTag>Artifact</tradeTag>
        <thingDefCountRange>2~2</thingDefCountRange>
        <countRange>1~1</countRange>
      </li>

      <!-- Slaves -->
      <!-- none -->
      
      <!-- Animals -->
      <li Class="StockGenerator_Animals">
        <tradeTagsSell>
          <li>AnimalFarm</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalExotic</li>
        </tradeTagsBuy>
        <kindCountRange>1~2</kindCountRange>
        <countRange>3~4</countRange>
        <createMatingPair>
          <li>AnimalFarm</li>
        </createMatingPair>
      </li>
      
      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      
    </stockGenerators>
  </TraderKindDef>

</Defs>
